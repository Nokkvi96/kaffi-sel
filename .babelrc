{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@constants": "./src/constants",
          "@hooks": "./src/hooks",
          "@modules": "./src/modules",
          "@pages": "./src/pages",
          "@recoil": "./src/recoil",
          "@styles": "./src/styles",
          "@utils": "./src/utils",
          "@public": "./public"
        }
      }
    ]
  ]
}
